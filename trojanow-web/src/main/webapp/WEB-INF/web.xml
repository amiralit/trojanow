  	<servlet-class>edu.trojanow.trojanowweb.AuthenticateService</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>profile</servlet-name>
      <url-pattern>/profile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Profile</servlet-name>
  	<url-pattern>/Profile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProfileService</servlet-name>
  	<url-pattern>/ProfileService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AuthenticateService</servlet-name>
  	<url-pattern>/AuthenticateService</url-pattern>
